# OpenAI API Key (required for wiki generation)
OPENAI_API_KEY="your_openai_api_key_here"

# Enable user-provided API keys (optional)
ENABLE_USER_API_KEYS="true"

# Free tier daily limit (optional, default: 10)
FREE_TIER_DAILY_LIMIT="10"

# Bypass usage limits for development/testing (optional, default: false)
# WARNING: Only use this for local development. Never enable in production!
BYPASS_USAGE_LIMITS="false"